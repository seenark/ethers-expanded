{
  "name": "@aumstack/ethers-expanded",
  "version": "0.0.27",
  "description": "wrap some ethers.js functions and hardhat utilities function",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "files": [
    "**/*"
  ],
  "exports": {
    ".": "./index.js",
    "./hardhat": {
      "types": "./hardhat/index.d.ts",
      "default": "./hardhat/index.js"
    },
    "./helper": {
      "types": "./helper/index.d.ts",
      "default": "./helper/index.js"
    },
    "./ethers": {
      "types": "./ethers/index.d.ts",
      "default": "./ethers/index.js"
    }
  },
  "private": false,
  "readme": "../README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/seenark/ethers-expanded"
  },
  "homepage": "https://github.com/seenark/ethers-expanded",
  "scripts": {
    "dev": "node -r esbuild-register src/index.ts"
  },
  "keywords": [
    "ethers",
    "hardhat"
  ],
  "author": "Atiwat Seenark",
  "license": "MIT",
  "dependencies": {
    "@nomiclabs/hardhat-ethers": "^2.2.0",
    "date-fns": "^2.29.3",
    "ethers": "^5.7.1",
    "fs-extra": "^10.1.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "esbuild": "^0.15.11",
    "esbuild-plugin-d.ts": "^1.1.0",
    "esbuild-register": "^3.3.3",
    "fast-glob": "^3.2.12"
  }
}
